<!DOCTYPE html>
<html>
<head>
	<%- partial('partials/header/header-start.ejs', {
			title: process.env.APP_NAME + (user ? ' - ' + user.name.full : '')
		}) %>
	<%- partial('partials/resources.ejs', {
			stylesheets: [
				'/public/lib/bootstrap/css/bootstrap.min.css',
				'/public/gf-style.css',
				'/public/landing.css'
			],
			scripts: []
		}) %>
	<%- partial('partials/header/header-end.ejs') %>
</head>
<body class="align-banner fix-banner">
	<%- partial('partials/topbar.ejs') %>

	<div class="container container-fluid">
		<div class="row-fluid over-banner">
			<img src="/public/images/brandlogo_01_134_134.png">
			<span class="title"><%= process.env.APP_NAME %></span>
		</div>

		<div class="row-fluid section">
			<div id="gf-login-outer" class="pull-left">
				<%- partial('partials/authboxes/loginbox.ejs', {
						parent: "#gf-login-outer"
					}) %>
			</div>
		</div>
	</div>
</body>
</html>
